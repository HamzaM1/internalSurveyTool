<mvc:View
	controllerName="demo.survey2.SurveyDemo2.controller.Quiz"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Shell>
		<Page
			title="{SQ_TITLE}"
			id="page"
			busy="{objectView>/busy}"
			busyIndicatorDelay="{objectView>/delay}"
			showNavButton="true"
			navButtonPress=".onNavBack">
			<content>
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<content>
						<!--Specfiy quiz name -->
						<Text
							class="myResults myHeader"
							text="{SQ_TITLE}: Non-Anon({ANONYMOUS}) {SQ_TYPE}"/>
					</content>
				</Panel>
				
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<content>
						<Text
							class="myResults myHeader"
							text="Questions:"/>
					</content> 
				</Panel>
				
				<List
				id="list"
				width="auto"
				items="{path:'/Questions'}"
				noDataText="{questionView>/tableNoDataText}"
				busyIndicatorDelay="{questionView>/tableBusyDelay}"
				growing="true" 
				updateFinished=".onUpdateFinished"
				class="myList">
					<items>
        				<ObjectListItem
        					title="{QUESTION_TITLE}"
        					type="Navigation"
        					press="onPress"/>
    				</items>
				</List>
				
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<Button 
						text="Submit"
						press=".onPressSubmit"/>
				</Panel>
			</content> 
		</Page>
	</Shell>
</mvc:View>