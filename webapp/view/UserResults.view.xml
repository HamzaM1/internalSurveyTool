<mvc:View controllerName="demo.survey2.SurveyDemo2.controller.UserResults" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Shell>
		<Page title="Quiz Results: User A" id="page" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}" showNavButton="true"
			navButtonPress=".onNavBack">
			<!-- specify user's name-->
			<headerContent class="header">
				<Button icon="sap-icon://home" press=".onPressHome"/>
			</headerContent>
			<content>
				<Panel backgroundDesign="Transparent">
					<Text class="myResults myHeader" text="Quiz: Non Anonymous Quiz"></Text>
					<!--Specfiy quiz name -->
					<Text class="myResults myHeader" text="Quiz Score: 100%"></Text>
					<!--Specfiy quiz score -->
				</Panel>
				<Panel  backgroundDesign="Transparent">
					<Text class="myResults myHeader" text="Question 1:"></Text>					<!--Specfiy quiz name -->
					
					<Text class="myResults myHeader" text="User Response:"></Text>	<!--Specfiy quiz name -->
						<Text class="myResults " text="Question 1"></Text>
					<!--Specfiy quiz name -->
					<RadioButtonGroup editable="false" class="myResults myInput">
						<RadioButton text="A"/>
						<RadioButton text="B"/>
						<RadioButton text="C"/>
						<RadioButton text="D"/>
					</RadioButtonGroup>
				</Panel>
			
			</content>
		</Page>
	</Shell>
</mvc:View>