<mvc:View controllerName="demo.survey2.SurveyDemo2.controller.UserResults" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc">
	<Shell>
		<Page title="Quiz Results: User A" id="page" busy="{objectView>/busy}" busyIndicatorDelay="{objectView>/delay}" showNavButton="true"
			navButtonPress=".onNavBack">
			<!-- specify user's name-->
			<headerContent class="header">
				<Button icon="sap-icon://home" press=".onPressHome"/>
			</headerContent>
			<content>
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<content>
						<!--Specfiy quiz name -->
						<Text
							class="myResults myHeader"
							text="Placeholder: Non Anonymous Placeholder"/>
						<!--Specfiy quiz score -->
						<Text
							class="myResults myHeader"
							text="Placeholder Score: Placeholder%"/>
					</content>
				</Panel>
				
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<content>
						<Text
							class="myResults myHeader"
							text="Question Placeholder:"/>
						<Text
							class="myResults myHeader"
							text="User Response:"/>
					</content> 
				</Panel>
				
				<Panel
					backgroundDesign="Transparent"
					class="sapUiResponsiveMargin">
					<content>
						<Text class="myResults myText"
							text="Placeholder"/>
						<RadioButtonGroup editable="false" class="myResults myInput">
							<RadioButton text="Placeholder"/>
							<RadioButton text="Placeholder"/>
							<RadioButton text="Placeholder"/>
						</RadioButtonGroup>
					</content> 
				</Panel>
			
			</content>
		</Page>
	</Shell>
</mvc:View>