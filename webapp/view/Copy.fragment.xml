<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<Dialog
		id="copyLink"
		title="Link to Copy">
		<content>
			<Text
				text="{SQ_LINK}"
				class="sapUiMediumMargin"/>
		</content>
		<beginButton>
			<Button
				text="Ok"
				press=".onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>