<mvc:View
	controllerName="demo.survey2.SurveyDemo2.controller.Quiz"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Shell>
		<Page
			title="{SNAME}"
			id="page"
			busy="{objectView>/busy}"
			busyIndicatorDelay="{objectView>/delay}"
			showNavButton="true"
			navButtonPress=".onNavBack">
			<headerContent
        		class="header">
				<Button
					icon="sap-icon://home"
					press=".onPressHome"/>
			</headerContent>
			<content>
			<Text
				text="Hello World"
				/>
			</content> 
		</Page>
	</Shell>
</mvc:View>